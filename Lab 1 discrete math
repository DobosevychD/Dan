#include <stdio.h>

int main()
{
    int x,y,z,a,b,c,d,e,h,k ;//створюємо змінні
    printf("x y z   (!x || !y)    (z == a)   (b == !z)   (x == y)    (d == c)   (x || y)   (e == h)   \n");//вивродимо логічні операції та значення x,y,z

    for (x=0;x<=1;x++)// для х=0 і х менший рівний 1, збільшуємо х на 1
        {
        for (y=0;y<=1;y++)// для у=0 і y менший рівний 1, збільшуємо у на 1
            {
            for (z=0;z<=1;z++)// для z=0 і z менший рівний 1, збільшуємо z на 1
            {
                a=(!x || !y);
                b=(z <= a);
                c=(b <= !z);
                d=(x == y);
                e=(d == c);
                h=(x || y);
                k=(e == h);
                printf("%d %d %d       %d           %d             %d           %d            %d           %d        %d\n",x,y,z,a,b,c,d,e,h,k);// Виводимо дані відповідно до логічних опер.
            }
            }
        }
    return 0;
}
